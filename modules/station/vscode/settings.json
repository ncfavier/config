{
  "editor.fontFamily": "monospace",
  "keyboard.dispatch": "keyCode",
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.view.pdf.viewer": "browser",
  "update.mode": "none",
  "workbench.colorCustomizations": {
    "[Lilac]": {
      "editorInlayHint.background": "#511b72"
    },
    "[White Night]": {
      "activityBar.foreground": "#555566",
      "editorLineNumber.foreground": "#333333",
      "editorLineNumber.activeForeground": "#777777",
      "input.placeholderForeground": "#555566",
      "panelTitle.activeBorder": "#555566",
      "peekViewTitleDescription.foreground": "#555566",
      "sideBarTitle.foreground": "#555566",
      "statusBar.foreground": "#555566",
      "tab.inactiveForeground": "#555566",
      "titleBar.inactiveForeground": "#555566",
      "gitDecoration.ignoredResourceForeground": "#555566",
      "gitDecoration.modifiedResourceForeground": "#dd00ff",
      "gitDecoration.untrackedResourceForeground": "#555566",

      "button.background": "#dd00ff",
      "editor.lineHighlightBackground": "#dd00ff14",
      "editor.selectionBackground": "#dd00ff7f",
      "editor.selectionHighlightBorder": "#dd00ff3f",
      "editor.wordHighlightBackground": "#dd00ff3f",
      "editor.wordHighlightStrongBackground": "#dd00ff7f",
      "editorBracketMatch.background": "#dd00ff7f",
      "editorCursor.foreground": "#dd00ff",
      "editorLink.activeForeground": "#dd00ff",
      "editorOverviewRuler.bracketMatchForeground": "#dd00ff7f",
      "editorOverviewRuler.selectionHighlightForeground": "#dd00ff7f",
      "editorOverviewRuler.wordHighlightForeground": "#dd00ff3f",
      "editorOverviewRuler.wordHighlightStrongForeground": "#dd00ff7f",
      "extensionButton.prominentBackground": "#dd00ff",
      "extensionButton.prominentHoverBackground": "#dd00ff",
      "list.dropBackground": "#dd00ff7f",
      "list.focusBackground": "#dd00ff3f",
      "list.highlightForeground": "#dd00ff",
      "progressBar.background": "#dd00ff",
      "terminal.ansiBrightMagenta": "#dd00ff",
      "terminal.ansiMagenta": "#dd00ff",
      "terminal.selectionBackground": "#dd00ff7f",
      "terminalCursor.foreground": "#dd00ff",
      "textLink.activeForeground": "#dd00ff",
      "textLink.foreground": "#dd00ff",
    }
  },
  "editor.tokenColorCustomizations": {
    "comments": "#aaaaaa",
    "[White Night]": {
      "comments": "#dd00ff"
    },
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["C-r"],
      "commands": ["redo"]
    }
  ],
  "coq.autoRevealProofStateAtCursor": true,
  "coq.format.indentAfterOpenProof": true,
  "coq.interpretToEndOfSentence": true,
  "coq.showProofViewOn": "open-script",
  "coqtop.startOn": "open-script",
  "diffEditor.codeLens": true,
  "editor.codeLensFontSize": 13,
  "haskell.plugin.tactics.globalOn": false,
  "haskell.updateBehavior": "never-check",
  "haskell.plugin.ghcide-type-lenses.globalOn": false,
  "haskell.plugin.hlint.globalOn": false,
  "editor.tabSize": 2,
  "search.exclude": {
    "**/_build": true,
    "**/.direnv": true,
    "**/.stack-work": true,
    "**/*.agdai": true,
    "**/*.hi": true,
    "**/*.o": true,
    "**/dist": true,
    "**/dist-*": true,
    "**/result": true,
    "**/result-*": true
  },
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*.vrb",
    "*.synctex.gz"
  ],
  "extensions.autoUpdate": false,
  "agdaMode.view.panelMountPosition": "right",
  "editor.codeLensFontFamily": "monospace",
  "files.insertFinalNewline": true,
  "editor.renderFinalNewline": "off",
  "files.trimFinalNewlines": true,
  "security.workspace.trust.enabled": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "‚Ñï": true,
    "‚Ñ§": true,
    "ùïä": true,
    "‚Ñö": true,
    "‚Ñù": true,
    "‚ÑÇ": true,
    "‚àß": true,
    "‚à®": true,
    "‚Ñì": true,
    "ùìû": true,
    "√ó": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "vim.useSystemClipboard": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.bracketPairs": "active",
  "extensions.autoCheckUpdates": false,
  "editor.minimap.enabled": false,
  "editor.selectionClipboard": false,
  "haskell.manageHLS": "PATH",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "latex-workshop.view.pdf.color.dark.backgroundColor": "",
  "latex-workshop.view.pdf.color.light.backgroundColor": "",
  "magicRacket.languageServer.enabled": false,
  "editor.wordWrap": "on",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/run/current-system/sw/bin/bash",
      "icon": "terminal-bash"
    },
  },
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "editor.occurrencesHighlight": "off",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": false,
    "<C-n>": false,
    "<C-p>": false,
    "<C-q>": false,
    "<C-s>": false,
    "<C-w>": false,
    "<C-z>": false,
  },
  "search.useGlobalIgnoreFiles": true,
  "window.openFilesInNewWindow": "default",
  "markdown.extension.toc.levels": "2..6",
  "haskell.formattingProvider": "fourmolu",
  "explorer.excludeGitIgnore": true,
  "editor.rulers": [
    72,
    85
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "xelatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-xelatex",
        "-outdir=%OUTDIR%",
        "%DOC%",
        "--shell-escape"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "latexmk",
      "args": [
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "rnw2tex",
      "command": "Rscript",
      "args": [
        "-e",
        "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
      ],
      "env": {}
    },
    {
      "name": "jnw2tex",
      "command": "julia",
      "args": [
        "-e",
        "using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
      ],
      "env": {}
    },
    {
      "name": "jnw2texminted",
      "command": "julia",
      "args": [
        "-e",
        "using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
      ],
      "env": {}
    },
    {
      "name": "pnw2tex",
      "command": "pweave",
      "args": [
        "-f",
        "tex",
        "%DOC_EXT%"
      ],
      "env": {}
    },
    {
      "name": "pnw2texminted",
      "command": "pweave",
      "args": [
        "-f",
        "texminted",
        "%DOC_EXT%"
      ],
      "env": {}
    },
    {
      "name": "tectonic",
      "command": "tectonic",
      "args": [
        "--synctex",
        "--keep-logs",
        "%DOC%.tex"
      ],
      "env": {}
    }
  ],
  "workbench.editor.empty.hint": "hidden",
  "rust-analyzer.cargo.features": "all",
  "haskell.plugin.importLens.codeActionsOn": false,
  "git.openRepositoryInParentFolders": "always",
  "editor.fontSize": 13,
  "lean4.alwaysShowTitleBarMenu": false,
  "lean4.automaticallyBuildDependencies": true,
  "files.trimTrailingWhitespace": true,
  "chat.commandCenter.enabled": false,
  "window.menuBarVisibility": "toggle",
  "window.customTitleBarVisibility": "never",
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autoStash": true,
  "git.fetchOnPull": true,
  "diffEditor.renderSideBySide": false,
  "agdaMode.connection.paths": [
    "agda"
  ],
  "editor.experimentalEditContextEnabled": false,
  "workbench.colorTheme": "Mr Pink",
}
