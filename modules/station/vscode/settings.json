{
  "editor.fontFamily": "JuliaMono",
  "files.trimTrailingWhitespace": true,
  "keyboard.dispatch": "keyCode",
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.view.pdf.viewer": "browser",
  "update.mode": "none",
  "workbench.colorCustomizations": {
    "[White Night]": {
      "activityBar.foreground": "#555566",
      "editorLineNumber.foreground": "#333333",
      "editorLineNumber.activeForeground": "#777777",
      "input.placeholderForeground": "#555566",
      "panelTitle.activeBorder": "#555566",
      "peekViewTitleDescription.foreground": "#555566",
      "sideBarTitle.foreground": "#555566",
      "statusBar.foreground": "#555566",
      "tab.inactiveForeground": "#555566",
      "titleBar.inactiveForeground": "#555566",
      "gitDecoration.ignoredResourceForeground": "#555566",
      "gitDecoration.modifiedResourceForeground": "#dd00ff",
      "gitDecoration.untrackedResourceForeground": "#555566",

      "button.background": "#dd00ff",
      "editor.lineHighlightBackground": "#dd00ff14",
      "editor.selectionBackground": "#dd00ff7f",
      "editor.selectionHighlightBorder": "#dd00ff3f",
      "editor.wordHighlightBackground": "#dd00ff3f",
      "editor.wordHighlightStrongBackground": "#dd00ff7f",
      "editorBracketMatch.background": "#dd00ff7f",
      "editorCursor.foreground": "#dd00ff",
      "editorLink.activeForeground": "#dd00ff",
      "editorOverviewRuler.bracketMatchForeground": "#dd00ff7f",
      "editorOverviewRuler.selectionHighlightForeground": "#dd00ff7f",
      "editorOverviewRuler.wordHighlightForeground": "#dd00ff3f",
      "editorOverviewRuler.wordHighlightStrongForeground": "#dd00ff7f",
      "extensionButton.prominentBackground": "#dd00ff",
      "extensionButton.prominentHoverBackground": "#dd00ff",
      "list.dropBackground": "#dd00ff7f",
      "list.focusBackground": "#dd00ff3f",
      "list.highlightForeground": "#dd00ff",
      "progressBar.background": "#dd00ff",
      "terminal.ansiBrightMagenta": "#dd00ff",
      "terminal.ansiMagenta": "#dd00ff",
      "terminal.selectionBackground": "#dd00ff7f",
      "terminalCursor.foreground": "#dd00ff",
      "textLink.activeForeground": "#dd00ff",
      "textLink.foreground": "#dd00ff",
    }
  },
  "editor.tokenColorCustomizations": {
    "[White Night]": {
      "comments": "#dd00ff"
    },
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["C-r"],
      "commands": ["redo"]
    }
  ],
  "haskell.plugin.importLens.codeLensOn": false,
  "coq.autoRevealProofStateAtCursor": true,
  "coq.format.indentAfterOpenProof": true,
  "coq.interpretToEndOfSentence": true,
  "coq.showProofViewOn": "open-script",
  "coqtop.startOn": "open-script",
  "diffEditor.codeLens": true,
  "editor.codeLensFontSize": 13,
  "haskell.plugin.tactics.globalOn": false,
  "haskell.updateBehavior": "never-check",
  "haskell.plugin.ghcide-type-lenses.globalOn": false,
  "haskell.plugin.hlint.diagnosticsOn": false,
  "editor.tabSize": 2,
  "nix.enableLanguageServer": true,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/result": true,
    "**/result-*": true
  },
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*.vrb",
    "*.synctex.gz"
  ],
  "window.menuBarVisibility": "hidden",
  "extensions.autoUpdate": false,
  "agdaMode.view.panelMountPosition": "right",
  "editor.codeLensFontFamily": "monospace",
  "files.insertFinalNewline": true,
  "editor.renderFinalNewline": false,
  "files.trimFinalNewlines": true,
  "workbench.editor.untitled.hint": "hidden",
  "security.workspace.trust.enabled": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "‚Ñï": true,
    "‚Ñ§": true,
    "ùïä": true,
    "‚Ñö": true,
    "‚Ñù": true,
    "‚ÑÇ": true,
    "‚àß": true,
    "‚à®": true,
    "‚Ñì": true,
    "ùìû": true,
    "√ó": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "vim.useSystemClipboard": true,
  "magic-racket.lsp.enabled": false,
  "files.exclude": {
    "**/*.agdai": true
  },
  "agdaMode.connection.agdaPath": "/etc/profiles/per-user/n/bin/agda",
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorTheme": "Mr Pink",
  "editor.fontSize": 12,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.bracketPairs": "active",
}
